           var mod = this.totalPageCount % this.pageRange;\r\n\t\t\t\tif (this.totalRecordCount === 0)\r\n\t\t\t\t{\r\n\t\t\t\t    return 0;\r\n\t\t\t\t}\r\n                else if (this.pageRangeIndex === this.maxRangeIndex && mod != 0) {\r\n                    return mod;\r\n                }\r\n                return this.pageRange;\r\n            }\r\n  },\r\n  methods: {\r\n    sortcolumn(val, col) {\r\n      return this.ShowList.sort((a, b) => (a[col] - b[col]) * val);\r\n    },\r\n  },\r\n  created() {\r\n    bus.on(\"plus\", (msg) => {\r\n      this.searchNameText = msg;\r\n    });\r\n  },\r\n  beforeUnmount() {\r\n    bus.off(\"plus\");\r\n  },\r\n};\r\n</script>    "]}]}